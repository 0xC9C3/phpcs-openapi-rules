{
	"name": "anon/phpcs-openapi-rules",
	"description": "OpenAPI Rules for PHP_CodeSniffer",
	"license": "MIT",
	"type": "phpcodesniffer-standard",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"bin-dir": "bin",
		"platform": {
			"php": "7.4.0"
		}
	},
	"require": {
		"php": "^7.1 || ^8.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.6.2 || ^0.7",
		"phpstan/phpdoc-parser": "0.5.1 - 0.5.5",
		"squizlabs/php_codesniffer": "^3.5.6",
		"zircote/swagger-php": "^3.2"
	},
	"require-dev": {
		"phing/phing": "2.16.4",
		"php-parallel-lint/php-parallel-lint": "1.3.0",
		"phpstan/phpstan": "0.12.90",
		"phpstan/phpstan-deprecation-rules": "0.12.6",
		"phpstan/phpstan-phpunit": "0.12.20",
		"phpstan/phpstan-strict-rules": "0.12.9",
		"phpunit/phpunit": "7.5.20|8.5.5|9.5.5"
	},
	"autoload": {
		"psr-4": {
			"OpenApiRules\\": "OpenApiRules"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"OpenApiRules\\PHPStan\\": "build/PHPStan",
			"OpenApiRules\\": "tests"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "7.x-dev"
		}
	}
}
